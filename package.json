{
  "name": "@dignetwork/chia-block-listener",
  "version": "1.0.0",
  "description": "Chia blockchain block listener with event emitter for Node.js",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/DIG-Network/chia-block-listene"
  },
  "napi": {
    "name": "chia-block-listener",
